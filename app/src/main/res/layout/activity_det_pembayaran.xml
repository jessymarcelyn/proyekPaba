<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#212122"
    tools:context=".activityOngoing">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/flTop"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="#C9F24D"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginStart="20dp"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:text="Skyfit Gym"
            android:textSize="30sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ivProfile"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginEnd="16dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView8"
            tools:srcCompat="@drawable/profile" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="323dp"
        android:layout_height="633dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flTop"
        app:srcCompat="@drawable/rounded_background" />

    <TextView
        android:id="@+id/tvidTransaksii"
        android:layout_width="241dp"
        android:layout_height="26dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textColor="@color/green"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toTopOf="@+id/imageView5" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Tanggal Transaksi "
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tvidTransaksii"
        app:layout_constraintTop_toBottomOf="@+id/tvidTransaksii" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="136dp"
        android:layout_height="19dp"
        android:layout_marginTop="7dp"
        android:text="Metode Transaksi "
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView15"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Total Transaksi"
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView16"
        app:layout_constraintTop_toBottomOf="@+id/textView16" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Pilihan Paket "
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Durasi Paket "
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView18"
        app:layout_constraintTop_toBottomOf="@+id/textView18" />

    <TextView
        android:id="@+id/tvMulaii"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Tanggal Mulai"
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/textView19" />

    <TextView
        android:id="@+id/tvBerakhirr"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Tanggal Berakhir"
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tvMulaii"
        app:layout_constraintTop_toBottomOf="@+id/tvMulaii" />

    <TextView
        android:id="@+id/TVTotalSesii"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Total Sesi"
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/tvBerakhirr"
        app:layout_constraintTop_toBottomOf="@+id/tvBerakhirr" />

    <TextView
        android:id="@+id/tvNamaTrainerTransaksii"
        android:layout_width="136dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:text="Nama Trainer"
        android:textColor="#F6F2F2"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/TVTotalSesii"
        app:layout_constraintTop_toBottomOf="@+id/TVTotalSesii" />

    <TextView
        android:id="@+id/tvTglTransaksi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView15"
        app:layout_constraintTop_toTopOf="@+id/textView15" />

    <TextView
        android:id="@+id/tvMetodeTransaksi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView16"
        app:layout_constraintTop_toTopOf="@+id/textView16" />

    <TextView
        android:id="@+id/tvTotalTransaksi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toTopOf="@+id/textView17" />

    <TextView
        android:id="@+id/tvPaket"
        android:layout_width="147dp"
        android:layout_height="42dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView18"
        app:layout_constraintTop_toTopOf="@+id/textView18" />

    <TextView
        android:id="@+id/tvDurasiTransaksi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toTopOf="@+id/textView19" />

    <TextView
        android:id="@+id/tvTglMulaiT"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/tvMulaii"
        app:layout_constraintTop_toTopOf="@+id/tvMulaii" />

    <TextView
        android:id="@+id/tvTglBerakhirT"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/tvBerakhirr"
        app:layout_constraintTop_toTopOf="@+id/tvBerakhirr" />

    <TextView
        android:id="@+id/TVTotalSesi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="TextView"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/TVTotalSesii"
        app:layout_constraintTop_toTopOf="@+id/TVTotalSesii" />

    <TextView
        android:id="@+id/tvNamaTrainerTransaksi"
        android:layout_width="201dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:textColor="@color/white"
        android:textSize="16sp"
        app:layout_constraintStart_toEndOf="@+id/tvNamaTrainerTransaksii"
        app:layout_constraintTop_toTopOf="@+id/tvNamaTrainerTransaksii" />




</androidx.constraintlayout.widget.ConstraintLayout>